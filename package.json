{
  "name": "lscontests",
  "version": "2.3.0",
  "description": "A tool to get information about the contests on each OJ",
  "keywords": [
    "cli",
    "crawler",
    "atcoder",
    "codeforces",
    "luogu",
    "nowcoder",
    "competitive-programming"
  ],
  "repository": "https://github.com/StableAgOH/lscontests",
  "bugs": "https://github.com/StableAgOH/lscontests/issues",
  "author": "StableAgOH <stagoh17@gmail.com>",
  "license": "GPL-3.0",
  "main": "./dist/index.js",
  "bin": {
    "lsct": "./dist/cli.js",
    "lsct-s": "./dist/server.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist/*",
    "postbuild": "cpy --flat src/server.html dist",
    "build": "tsc -p src",
    "clean": "rimraf ./dist ./docs ./node_modules",
    "docs": "typedoc",
    "lint": "eslint . && markdownlint .",
    "lint:fix": "eslint . --fix && markdownlint . --fix",
    "start": "tsx src/cli.ts",
    "start-s": "tsx src/server.ts"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "axios-retry": "^3.2.5",
    "cheerio": "^1.0.0-rc.11",
    "commander": "^9.2.0",
    "express": "^4.18.1",
    "ics": "^2.35.0",
    "lodash": "^4.17.21",
    "pangu": "^4.0.7",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.35",
    "@types/pangu": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "cpy-cli": "^4.1.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.16.0",
    "markdownlint-cli": "^0.31.1",
    "rimraf": "^3.0.2",
    "tsx": "^3.4.1",
    "typedoc": "^0.22.15",
    "typedoc-plugin-extras": "^2.2.3",
    "typescript": "4.6.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
