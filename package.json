{
  "name": "lscontests",
  "version": "2.6.3",
  "description": "A tool to get information about the contests on each OJ",
  "keywords": [
    "cli",
    "crawler",
    "atcoder",
    "codeforces",
    "luogu",
    "nowcoder",
    "competitive-programming"
  ],
  "repository": "https://github.com/StableAgOH/lscontests",
  "bugs": "https://github.com/StableAgOH/lscontests/issues",
  "author": "StableAgOH <stagoh17@gmail.com>",
  "license": "GPL-3.0",
  "main": "./dist/index.js",
  "bin": {
    "lsct": "./dist/cli.js",
    "lsct-s": "./dist/server.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist/*",
    "postbuild": "cpy --flat src/server.html dist",
    "build": "tsc -p src",
    "clean": "rimraf ./dist ./docs ./node_modules",
    "docs": "typedoc",
    "lint": "eslint . && markdownlint .",
    "lint:fix": "eslint . --fix && markdownlint . --fix",
    "start": "tsx src/cli.ts",
    "start-s": "tsx src/server.ts"
  },
  "dependencies": {
    "await-lock": "^2.2.2",
    "axios": "^1.4.0",
    "axios-retry": "^3.5.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.0.0",
    "express": "^4.18.2",
    "html-entities": "^2.3.6",
    "ics": "^3.2.0",
    "lodash": "^4.17.21",
    "pangu": "^4.0.7",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.1",
    "@types/pangu": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "cpy-cli": "^4.2.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.43.0",
    "markdownlint-cli": "^0.35.0",
    "rimraf": "^5.0.1",
    "tsx": "^3.12.7",
    "typedoc": "^0.24.8",
    "typedoc-plugin-extras": "^2.3.3",
    "typescript": "^5.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
